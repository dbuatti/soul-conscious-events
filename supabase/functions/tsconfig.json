{
  "compilerOptions": {
    "lib": ["esnext", "deno.ns"],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      // Paths are typically for local aliases, not remote URLs
    }
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}