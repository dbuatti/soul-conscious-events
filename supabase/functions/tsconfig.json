{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["deno.ns", "esnext"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "types": ["deno"]
  },
  "include": ["**/*.ts"]
}